{
  "name": "@data-heaving/source-sql-mssql",
  "version": "0.9.0",
  "author": {
    "name": "Stanislav Muhametsin",
    "email": "346799+stazz@users.noreply.github.com",
    "url": "https://github.com/stazz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DataHeaving/source-sql.git"
  },
  "scripts": {
    "build": "npm run tsc && npm run lint",
    "tsc": "tsc --project tsconfig.json && tsc --project tsconfig.json --outDir ./dist/esm --module ES2020",
    "prepublishOnly": "rm -rf dist && npm run build && npm run test",
    "lint": "eslint ./src --ext .ts,.tsx",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "run": "node dist/cjs",
    "dev-run": "ts-node dist/cjs",
    "test": "npm run run-test && npm run remove-test-files",
    "run-test": "ava",
    "remove-test-files": "rm -rf dist/*/tests dist/*/test-setup"
  },
  "dependencies": {
    "@data-heaving/source-sql": "0.9.0",
    "mssql": "6.3.1"
  },
  "devDependencies": {
    "@types/mssql": "6.0.7",
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "ava": "4.0.0-alpha.2",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "ava": {
    "files": [
      "dist/cjs/**/tests/*.js"
    ],
    "require": [
      "./dist/cjs/tests-setup"
    ]
  }
}
